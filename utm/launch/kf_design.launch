<launch>
<!--Launches multiple kalman filters to tune Q-->


    <group ns="qe1">
        <include file="$(find utm)/launch/kf.launch">
            <arg name="Q_param" value="0.1"/>
            <arg name="R_param" value="0.1"/>
        </include>
    </group>

    <group ns="qe2">
        <include file="$(find utm)/launch/kf.launch">
            <arg name="Q_param" value="0.2"/>
            <arg name="R_param" value="0.2"/>
        </include>
    </group>

    <group ns="qe3">
        <include file="$(find utm)/launch/kf.launch">
            <arg name="Q_param" value="0.3"/>
            <arg name="R_param" value="0.3"/>
        </include>
    </group>

    <group ns="qe4">
        <include file="$(find utm)/launch/kf.launch">
            <arg name="Q_param" value="0.4"/>
            <arg name="R_param" value="0.4"/>
        </include>
    </group>

    <group ns="qe5">
        <include file="$(find utm)/launch/kf.launch">
            <arg name="Q_param" value="0.5"/>
            <arg name="R_param" value="0.5"/>
        </include>
    </group>

    <group ns="qe6">
        <include file="$(find utm)/launch/kf.launch">
            <arg name="Q_param" value="0.6"/>
            <arg name="R_param" value="0.6"/>
        </include>
    </group>


    <group ns="qe7">
        <include file="$(find utm)/launch/kf.launch">
            <arg name="Q_param" value="0.7"/>
